{
    "name": "@jwalton/create-ts-app",
    "version": "1.0.7",
    "description": "",
    "author": {
        "name": "Jason Walton",
        "email": "dev@lucid.thedreaming.org"
    },
    "license": "MIT",
    "sideEffects": false,
    "main": "./dist/index.js",
    "bin": {
        "create-ts-app": "./dist/index.js"
    },
    "type": "module",
    "files": [
        "dist/**/*",
        "template/**/*"
    ],
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "test": "npm run build && npm run lint && npm run test:unittest",
        "build": "swc ./src -d dist --strip-leading-paths --ignore '**/*.spec.ts'",
        "clean": "rm -rf dist types coverage",
        "test:unittest": "nyc mocha",
        "lint": "npm run lint:source && npm run lint:types",
        "lint:source": "eslint src",
        "lint:types": "tsc --noEmit && tsc -p tsconfig.test.json --noEmit",
        "prepare": "husky || true && npm run build",
        "prepublishOnly": "npm run build && npm test",
        "semantic-release": "semantic-release"
    },
    "lint-staged": {
        "**/*.@(ts|tsx)}": [
            "prettier --write",
            "eslint"
        ],
        "**/*.@(js|cjs|mjs|jsx)}": [
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jwalton/create-ts-app.git"
    },
    "keywords": [
        ""
    ],
    "bugs": {
        "url": "https://github.com/exegesis-js/jwalton/create-ts-app/issues"
    },
    "homepage": "https://github.com/jwalton/create-ts-app#readme",
    "devDependencies": {
        "@eslint/js": "^9.1.1",
        "@swc/cli": "^0.3.9",
        "@swc/core": "^1.4.2",
        "@types/chai": "^4.3.12",
        "@types/express": "^4.17.21",
        "@types/inquirer-npm-name": "^3.0.7",
        "@types/lodash": "^4.14.202",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.4.1",
        "@types/sinon": "^17.0.3",
        "@types/yargs": "^17.0.32",
        "chai": "^5.1.0",
        "eslint": "^8.44.0",
        "eslint-plugin-import": "^2.26.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "mocha": "^10.3.0",
        "nyc": "^15.1.0",
        "prettier": "^3.2.5",
        "sinon": "^17.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.1.6",
        "typescript-eslint": "^7.8.0"
    },
    "dependencies": {
        "inquirer": "^9.2.12",
        "inquirer-npm-name": "^4.0.0",
        "lodash": "^4.17.21",
        "yargs": "^17.7.2"
    }
}
